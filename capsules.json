{
  "include": [
    "promisedio_uv.h"
  ],
  "modules": {
    "promisedio/fs": {
      "output": "capsule/promisedio/fs.h",
      "export": "fs_export.h"
    },
    "promisedio/loop": {
      "output": "capsule/promisedio/loop.h",
      "export": "loop_export.h"
    },
    "promisedio/ns": {
      "output": "capsule/promisedio/ns.h",
      "export": "ns_export.h",
      "extend": [
        "ns_defs.h"
      ]
    },
    "promisedio/signal": {
      "output": "capsule/promisedio/signal.h",
      "export": "signal_export.h"
    },
    "promisedio/timer": {
      "output": "capsule/promisedio/timer.h",
      "export": "timer_export.h",
      "extend": [
          "timer_defs.h",
          "timer_capsule.h"
      ]
    }
  }
}
